{
  "name": "scotch-react-router",
  "version": "1.0.0",
  "description": "React Router Aricle for Scotch",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack",
    "serve": "serve ./public",
    "start": "nodemon",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "buildall": "npm run build:client && npm run build:server"
  },
  "author": "Christian Nwamba",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "serve": "^1.4.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "redux-devtools": "^3.3.2",
    "http-server": "^0.8.5"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "if-env": "^1.0.0",
    "material-ui": "^1.0.0-beta.4",
    "material-ui-icons": "^1.0.0-alpha.19",
    "material-ui-responsive-drawer": "^1.1.31",
    "material-ui-superselectfield": "^1.5.4",
    "prop-types": "latest",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-infinite": "latest",
    "react-redux": "^5.0.2",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-responsive": "^4.3.2",
    "redux-thunk": "^2.2.0"
  }
}
